    "default_actions": [
      "$gt0113|Clonal BM plasma cells %|=3|local::at0088|Unknown|",
      "$gt0121|Plasmacytoma|=2|local::at0087|Unknown|",
      "$gt0125|Anaemia|=3|local::at0090|Unknown|",
      "$gt0116|Hypercalcaemia|=3|local::at0079|Unknown|",
      "$gt0115|Renal Insufficiency|=3|local::at0089|Unknown|",
      "$gt0128|Bone lesions|=2|local::at0097|Unknown|",
      "$gt0129|Evidence of end organ damage|=2|local::at0095|Unknown|",
      "$gt0131|Focal lesion on MRI studies|=2|local::at0092|Unknown|",
      "$gt0130|Serum-free light chain ratio|=2|local::at0096|Unknown|",
      "$gt0134|Biomarkers of malignancy|=2|local::at0093|Unknown|",
      "$gt0122|Multiple myeloma diagnosis|=2|local::at0094|Unknown|"
    ],
    "rules": {
      "gt0135": {
        "id": "gt0135",
        "priority": 26,
        "when": [
          "$gt0160|Clonal BMPC result|<10,%"
        ],
        "then": [
          "$gt0113|Clonal BM plasma cells %|=0|local::at0037|less than 10%|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 25,
        "when": [
          "$gt0160|Clonal BMPC result|>=10,%",
          "$gt0160|Clonal BMPC result|<60,%"
        ],
        "then": [
          "$gt0113|Clonal BM plasma cells %|=1|local::at0038|between 10% and 59%|"
        ]
      },
      "gt0081": {
        "id": "gt0081",
        "priority": 24,
        "when": [
          "$gt0160|Clonal BMPC result|>=60,%"
        ],
        "then": [
          "$gt0113|Clonal BM plasma cells %|=2|local::at0070|60% or more|"
        ]
      },
      "gt0153": {
        "id": "gt0153",
        "priority": 23,
        "when": [
          "($gt0198|Solitary plasmacytoma of bone|!=null)||(($gt0200|Extramedullary plasmacytoma|!=null)||($gt0213|Plasmacytoma|==1|local::at0040|Present - Biopsy-proven bony or extramedullary|))"
        ],
        "then": [
          "$gt0121|Plasmacytoma|=1|local::at0040|Present - Biopsy-proven bony or extramedullary|"
        ]
      },
      "gt0155": {
        "id": "gt0155",
        "priority": 22,
        "when": [
          "$gt0198|Solitary plasmacytoma of bone|==null",
          "$gt0200|Extramedullary plasmacytoma|==null",
          "$gt0213|Plasmacytoma|==0|local::at0039|Absent|"
        ],
        "then": [
          "$gt0121|Plasmacytoma|=0|local::at0039|Absent|"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 21,
        "when": [
          "($gt0166|Calcium result|>2.75,mmol/L)||($gt0166|Calcium result|>11,mg/dl)"
        ],
        "then": [
          "$gt0116|Hypercalcaemia|=1|local::at0078|Present (serum calcium > 2.75 mmol/L (> 11 mg/dL))|"
        ]
      },
      "gt0144": {
        "id": "gt0144",
        "priority": 20,
        "when": [
          "($gt0166|Calcium result|<=2.75,mmol/L)||($gt0166|Calcium result|<=11,mg/dl)"
        ],
        "then": [
          "$gt0116|Hypercalcaemia|=0|local::at0043|Absent|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 19,
        "when": [
          "$gt0169|Haemoglobin result|<100,g/L"
        ],
        "then": [
          "$gt0125|Anaemia|=2|local::at0081|Present (haemoglobin < 100 g/L)|"
        ]
      },
      "gt0143": {
        "id": "gt0143",
        "priority": 18,
        "when": [
          "$gt0169|Haemoglobin result|>=100,g/L"
        ],
        "then": [
          "$gt0125|Anaemia|=0|local::at0045|Absent|"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 17,
        "when": [
          "$gt0161|CrCl result|<40,ml/min"
        ],
        "then": [
          "$gt0115|Renal Insufficiency|=1|local::at0042|Present (CrCl < 40 mL/min)|"
        ]
      },
      "gt0111": {
        "id": "gt0111",
        "priority": 16,
        "when": [
          "($gt0178|Serum creatinine result|>177,umol/L)||($gt0178|Serum creatinine result|>2,mg/dl)"
        ],
        "then": [
          "$gt0115|Renal Insufficiency|=2|local::at0080|Present (serum creatinine > 177 umol/L (>2 mg/dL))|"
        ]
      },
      "gt0145": {
        "id": "gt0145",
        "priority": 15,
        "when": [
          "(($gt0161|CrCl result|>=40,ml/min)||($gt0178|Serum creatinine result|<=177,umol/L))||($gt0178|Serum creatinine result|<=2,mg/dl)"
        ],
        "then": [
          "$gt0115|Renal Insufficiency|=0|local::at0041|Absent|"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 14,
        "when": [
          "$gt0208|total osteolytic lesions|>=1"
        ],
        "then": [
          "$gt0128|Bone lesions|=1|local::at0048|one or more|"
        ]
      },
      "gt0214": {
        "id": "gt0214",
        "priority": 13,
        "when": [
          "$gt0208|total osteolytic lesions|<1"
        ],
        "then": [
          "$gt0128|Bone lesions|=0|local::at0047|zero|"
        ]
      },
      "gt0147": {
        "id": "gt0147",
        "priority": 12,
        "when": [
          "$gt0177|Involved/uninvolved FLC ratio|<100"
        ],
        "then": [
          "$gt0130|Serum-free light chain ratio|=0|local::at0073|lower than 100|"
        ]
      },
      "gt0095": {
        "id": "gt0095",
        "priority": 11,
        "when": [
          "$gt0177|Involved/uninvolved FLC ratio|>=100"
        ],
        "then": [
          "$gt0130|Serum-free light chain ratio|=1|local::at0074|100 or higher|"
        ]
      },
      "gt0092": {
        "id": "gt0092",
        "priority": 10,
        "when": [
          "$gt0205|total focal lesions (size min. 5mm)|>=2"
        ],
        "then": [
          "$gt0131|Focal lesion on MRI studies|=1|local::at0084|two or more (5 mm or greater in size)|"
        ]
      },
      "gt0132": {
        "id": "gt0132",
        "priority": 9,
        "when": [
          "$gt0205|total focal lesions (size min. 5mm)|<=1"
        ],
        "then": [
          "$gt0131|Focal lesion on MRI studies|=0|local::at0076|zero or one (5 mm or greater in size)|"
        ]
      },
      "gt0075": {
        "id": "gt0075",
        "priority": 8,
        "when": [
          "(($gt0125|Anaemia|==1|local::at0046|Present (haemoglobin > 20 g/L below the lower limit of normal)|)||(($gt0125|Anaemia|==2|local::at0081|Present (haemoglobin < 100 g/L)|)||(($gt0116|Hypercalcaemia|==1|local::at0078|Present (serum calcium > 2.75 mmol/L (> 11 mg/dL))|)||(($gt0115|Renal Insufficiency|==1|local::at0042|Present (CrCl < 40 mL/min)|)||($gt0115|Renal Insufficiency|==2|local::at0080|Present (serum creatinine > 177 umol/L (>2 mg/dL))|)))))||($gt0128|Bone lesions|==1|local::at0048|one or more|)",
          "($gt0113|Clonal BM plasma cells %|==1|local::at0038|between 10% and 59%|)||(($gt0113|Clonal BM plasma cells %|==2|local::at0070|60% or more|)||($gt0121|Plasmacytoma|==1|local::at0040|Present - Biopsy-proven bony or extramedullary|))"
        ],
        "then": [
          "$gt0129|Evidence of end organ damage|=1|local::at0050|Present|"
        ]
      },
      "gt0150": {
        "id": "gt0150",
        "priority": 7,
        "when": [
          "$gt0115|Renal Insufficiency|==0|local::at0041|Absent|",
          "$gt0116|Hypercalcaemia|==0|local::at0043|Absent|",
          "$gt0125|Anaemia|==0|local::at0045|Absent|",
          "$gt0128|Bone lesions|==0|local::at0047|zero|"
        ],
        "then": [
          "$gt0129|Evidence of end organ damage|=0|local::at0049|Absent|"
        ]
      },
      "gt0085": {
        "id": "gt0085",
        "priority": 6,
        "when": [
          "($gt0113|Clonal BM plasma cells %|==2|local::at0070|60% or more|)||(($gt0131|Focal lesion on MRI studies|==1|local::at0084|two or more (5 mm or greater in size)|)||($gt0130|Serum-free light chain ratio|==1|local::at0074|100 or higher|))"
        ],
        "then": [
          "$gt0134|Biomarkers of malignancy|=1|local::at0052|Present|"
        ]
      },
      "gt0154": {
        "id": "gt0154",
        "priority": 5,
        "when": [
          "($gt0113|Clonal BM plasma cells %|==1|local::at0038|between 10% and 59%|)||($gt0113|Clonal BM plasma cells %|==0|local::at0037|less than 10%|)",
          "$gt0130|Serum-free light chain ratio|==0|local::at0073|lower than 100|",
          "$gt0131|Focal lesion on MRI studies|==0|local::at0076|zero or one (5 mm or greater in size)|"
        ],
        "then": [
          "$gt0134|Biomarkers of malignancy|=0|local::at0051|Absent|"
        ]
      },
      "gt0072": {
        "id": "gt0072",
        "priority": 4,
        "when": [
          "$gt0113|Clonal BM plasma cells %|==0|local::at0037|less than 10%|",
          "$gt0121|Plasmacytoma|==0|local::at0039|Absent|"
        ],
        "then": [
          "$gt0122|Multiple myeloma diagnosis|=0|local::at0053|Negative|"
        ]
      },
      "gt0151": {
        "id": "gt0151",
        "priority": 3,
        "when": [
          "$gt0129|Evidence of end organ damage|==0|local::at0049|Absent|",
          "$gt0134|Biomarkers of malignancy|==0|local::at0051|Absent|"
        ],
        "then": [
          "$gt0122|Multiple myeloma diagnosis|=0|local::at0053|Negative|"
        ]
      },
      "gt0149": {
        "id": "gt0149",
        "priority": 2,
        "when": [
          "($gt0113|Clonal BM plasma cells %|==1|local::at0038|between 10% and 59%|)||(($gt0113|Clonal BM plasma cells %|==2|local::at0070|60% or more|)||($gt0121|Plasmacytoma|==1|local::at0040|Present - Biopsy-proven bony or extramedullary|))",
          "$gt0129|Evidence of end organ damage|==1|local::at0050|Present|"
        ],
        "then": [
          "$gt0122|Multiple myeloma diagnosis|=1|local::at0054|Positive|"
        ]
      },
      "gt0152": {
        "id": "gt0152",
        "priority": 1,
        "when": [
          "($gt0113|Clonal BM plasma cells %|==1|local::at0038|between 10% and 59%|)||(($gt0113|Clonal BM plasma cells %|==2|local::at0070|60% or more|)||($gt0121|Plasmacytoma|==1|local::at0040|Present - Biopsy-proven bony or extramedullary|))",
          "$gt0134|Biomarkers of malignancy|==1|local::at0052|Present|",
          "$gt0129|Evidence of end organ damage|==0|local::at0049|Absent|"
        ],
        "then": [
          "$gt0122|Multiple myeloma diagnosis|=1|local::at0054|Positive|"
        ]
      }
    }
